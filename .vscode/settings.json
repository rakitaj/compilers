{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.cache": true,
        "**/__pycache__": true,
        "**/.mypy_cache": true,
        "**/venv": true
    },
    "python.pythonPath": "${workspaceRoot}\\venv\\Scripts\\python.exe",
    "python.unitTest.pyTestEnabled": true
}